//
// Legacy Default Public
//
// !!! IMPORTANT NOTE !!!
// Ensure all values are surrounded by quotes, the parser can't properly deal with negative values otherwise.

configname "^1Legacy ^*Public"

public

init
{
	set g_altStopwatchMode "0"
	set g_complaintlimit "6"
	set g_ipcomplaintlimit "3"
	set g_doWarmup "0"
	set g_maxlives "0"
	set g_teamforcebalance "1"
	set g_noTeamSwitching "0"
	set g_lms_teamForceBalance "1"
	set g_voicechatsallowed "5"
	set g_warmup "30"
	set match_latejoin "1"
	set match_timeoutcount "0"
	set match_warmupDamage "0"
	set team_maxplayers "0"
	set team_nocontrols "1"
	set sv_allowDownload "1"
	set sv_floodProtect "0"
	set sv_fps "20"
	set vote_limit "5"

	set team_riflegrenades "1"

	set g_pronedelay "0"
	set g_shove "60"

	set g_antiwarp "1"
	set g_fixedphysics "1"
	set g_fixedphysicsfps "125"

	// reset cvars which might be set (but no changed) when switching from public to comp configs
	set g_userTimeLimit "0"
	set g_useralliedrespawntime "0"
	set g_useraxisrespawntime "0"
	set g_moverScale "1.0"

	//command "sv_cvarempty"
	command "sv_cvar rate EQ 25000"
	command "sv_cvar cl_maxpackets EQ 125"
	command "sv_cvar snaps EQ 20"
	command "sv_cvar com_maxfps IN 40 125"
}
