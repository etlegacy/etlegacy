ui/assets/etl_logo_huge_dark
{
	nomipmaps
	nopicmip
	{
		clampmap ui/assets/etl_logo_huge.tga
		blendfunc blend
		alphaGen const 0.25
	}
}

ui/assets/etl_logo
{
	nomipmaps
	nopicmip
